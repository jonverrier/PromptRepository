  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.8",
    "@types/sinon": "^17.0.4",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "expect": "^29.7.0",
    "mocha": "^11.0.1",
    "openai": "^4.89.1",
    "rimraf": "^6.0.1",
    "rimraff": "^0.0.1-security",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  }
}
