{
  "name": "promptrepository",
  "version": "0.1.0",
  "description": "Managing Prompts and standardised Unit Evals ",
  "main": "src/entry.ts",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "author": "Jon",
  "license": "MIT",
  "scripts": {
    "start": "ts-node entry.ts",
    "build": "tsc -b",
